{
  "name": "tournament",
  "version": "0.3.0",
  "description": "Simple tournament management",
  "author": "GHIFARI160",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ghifari160/tournament.git"
  },
  "scripts": {
    "start": "node src/server",
    "build:webpack:admin": "cd src/admin && npm run build",
    "build:webpack:public": "cd src/public && npm run build",
    "build:webpack": "npm run build:webpack:admin && npm run build:webpack:public",
    "build:rmsrc": "rm -r src/{admin,common,public}",
    "build:rmnode": "rm -rf node_modules",
    "build": "npm run build:webpack && npm run build:rmsrc && npm run build:rmnode"
  },
  "bin": {
    "tournament": "./src/server"
  },
  "devDependencies": {},
  "dependencies": {
    "marked": "^1.1.0",
    "mime-db": "^1.44.0",
    "mime-type": "^4.0.0",
    "semver": "^7.3.2"
  }
}
